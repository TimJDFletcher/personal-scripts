config wifi-device  wlan0
	option type     mac80211
	option channel  6 
	option macaddr  f8:d1:11:f7:01:42
	option hwmode   11ng
	option htmode   HT20
	list ht_capab   SHORT-GI-20
	list ht_capab   SHORT-GI-40
	list ht_capab   RX-STBC1
	list ht_capab   DSSS_CCK-40

config wifi-iface
	option device   wlan0
	option network  wan
#	option mode     sta
#	option ssid     TOKI
	option mode	ap
	option ssid	TLINK
	option encryption none


